{
  "name": "@sxzz/create",
  "version": "0.7.0",
  "packageManager": "pnpm@8.15.7",
  "description": "Command-line for creating projects from templates.",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "create": "./bin/create.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "dev": "tsx ./src/cli.ts",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.7",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "consola": "^3.2.3",
    "escape-string-regexp": "^5.0.0",
    "execa": "^8.0.1",
    "find-up": "^7.0.0",
    "giget": "^1.2.3",
    "js-yaml": "^4.1.0",
    "prompts": "^2.4.2",
    "replace-in-file": "^7.1.0",
    "unconfig": "^0.3.13"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^3.9.1",
    "@sxzz/prettier-config": "^2.0.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.7",
    "@types/prompts": "^2.4.9",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "eslint-define-config": "^2.1.0",
    "fast-glob": "^3.3.2",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prettier": "@sxzz/prettier-config"
}
