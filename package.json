{
  "name": "@sxzz/create",
  "version": "0.6.0",
  "packageManager": "pnpm@7.9.5",
  "description": "Command-line for creating projects from templates.",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "create": "./bin/create.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "dev": "tsx ./src/cli.ts",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.5",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "consola": "^3.2.3",
    "escape-string-regexp": "^5.0.0",
    "execa": "^7.2.0",
    "find-up": "^6.3.0",
    "giget": "^1.1.2",
    "js-yaml": "^4.1.0",
    "prompts": "^2.4.2",
    "replace-in-file": "^7.0.1",
    "unconfig": "^0.3.10"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^3.3.2",
    "@sxzz/prettier-config": "^1.0.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.4.5",
    "@types/prompts": "^2.4.4",
    "bumpp": "^9.1.1",
    "eslint": "^8.46.0",
    "eslint-define-config": "^1.22.0",
    "fast-glob": "^3.3.1",
    "prettier": "^3.0.0",
    "tsup": "^8.0.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
