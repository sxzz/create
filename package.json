    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "replace-in-file": "^8.4.0",
    "tsdown": "^0.18.3",
    "typescript": "^5.9.3",
    "unplugin-unused": "^0.5.6",
    "unrun": "^0.2.21"
  },
  "prettier": "@sxzz/prettier-config"
