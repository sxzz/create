{
  "name": "@sxzz/create",
  "version": "0.15.1",
  "packageManager": "pnpm@10.13.1",
  "description": "Command-line for creating projects from templates.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/create#readme",
  "bugs": {
    "url": "https://github.com/sxzz/create/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/create.git"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "create": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsx ./src/cli.ts",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "ansis": "^4.1.0",
    "args-tokenizer": "^0.3.0",
    "cac": "^6.7.14",
    "consola": "^3.4.2",
    "escape-string-regexp": "^5.0.0",
    "find-up-simple": "^1.0.1",
    "giget": "^2.0.0",
    "js-yaml": "^4.1.0",
    "tinyexec": "^1.0.1",
    "unconfig": "^7.3.2"
  },
  "devDependencies": {
    "@antfu/utils": "^9.2.0",
    "@sxzz/eslint-config": "^7.0.6",
    "@sxzz/prettier-config": "^2.2.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.16.5",
    "bumpp": "^10.2.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "replace-in-file": "^8.3.0",
    "tsdown": "^0.13.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unplugin-unused": "^0.5.1"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
